{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "paths": {
      "helpers/*": ["helpers/*"],
      "hook/*": ["hook/*"],
      "assets/*": ["assets/*"],
      "navigation/*": ["navigation/*"]
    }
  },
  "extends": "expo/tsconfig.base"
}
